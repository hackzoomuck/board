<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spring.mvc.board.mapper.PostMapper">
  <select id="findByTitle" parameterType="String" resultType="Post">
    SELECT * FROM post WHERE delete_whether = FALSE  AND title LIKE #{title}
  </select>
</mapper>
